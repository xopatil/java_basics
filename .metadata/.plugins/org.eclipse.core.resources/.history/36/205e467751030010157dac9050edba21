package q1;

class Emp{
	String name;
	int id;
	double sal;
	String address;
	
	public Emp(String name, int id, double sal, String address) {
		super();
		this.name = name;
		this.id = id;
		this.sal = sal;
		this.address = address;
	}
	
	
	
}







public class Main {
	public static void main(String[] args) {
		Emp emp = new Emp("tom", 120, 2220, "borivali");
		System.out.println(emp);
		
	}

}
