package overload_overrride;


class Account{
	double calsal(double sal) {
		return sal;
	}
	double calsal(double sal,double bonus) {
		return sal + bonus;
	}
	double calsal(double sal , double bonus , double tax) {
		return sal +bonus-tax;
	}
	
}

class Manager extends Account{
	
}

public class Main {

}
