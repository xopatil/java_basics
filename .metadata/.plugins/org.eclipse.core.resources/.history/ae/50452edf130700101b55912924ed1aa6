package q1a;
import static java.lang.System.*;


class A{
	
	static void meth() {
		System.out.println("this is the static method ");
	}
}


public class Importstatic {
	public static void main(String[] args) {

		out.println("this is static method");
		
	}
	public static void meth() {
		System.out.println("this is the static method ");
	}

}
