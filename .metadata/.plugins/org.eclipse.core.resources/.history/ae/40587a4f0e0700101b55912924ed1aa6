package chapter10;

public class Stringclasses {
	public static void main(String[] args) {
		
		String s1 = "soham";
		String s2 = "tejas";
		
		System.out.println(s1.length());
		System.out.println(s1.compareTo(s2));
		System.out.println(s1.concat(s2));
		System.out.println(s1.trim());
		
		
		
	}

}
