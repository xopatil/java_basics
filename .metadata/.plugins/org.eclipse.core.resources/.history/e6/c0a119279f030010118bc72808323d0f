package Q3p1;

class A {
	public int a = 10;
	private int b = 20;
	static int c = 30;
	int d = 40;

	public static void main(String[] args) {
		A a = new A();
		System.out.println(a.a);
		System.out.println(a.b);
		System.out.println(A.c);
		System.out.println(a.d);

	}

}

class B {

}

class C extends A {

}

public class Main {

}
